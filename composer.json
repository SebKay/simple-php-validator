{
    "name": "sebkay/simple-php-validator",
    "authors": [
        {
            "name": "Seb Kay",
            "email": "seb@sebkay.com",
            "homepage": "https://sebkay.com/"
        }
    ],
    "type": "project",
    "autoload": {
        "psr-4": {
            "SebKay\\SPV\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/pint": "^1.6"
    },
    "scripts": {
        "build": [
            "@clean",
            "composer install"
        ],
        "lint:php": "./vendor/bin/pint --test",
        "fix:php": "./vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    }
}
